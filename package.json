    "sass-loader": "^10.0.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
